# - Config file for the OCLINT_CORE package
# It defines the following variables
#  OCLINT_RULES_INCLUDE_DIRS - include directories for OCLINT_CORE
#  OCLINT_RULES_LIBRARIES    - libraries to link against
#  OCLINT_RULES_EXECUTABLE   - the bar executable
 
# Compute paths
get_filename_component(OCLINT_RULES_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(OCLINT_RULES_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")
 
# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET OCLintAbstractRule AND NOT OCLINT_RULES_BINARY_DIR)
  include("${OCLINT_RULES_CMAKE_DIR}/OCLintRulesTargets.cmake")
endif()
 
# These are IMPORTED targets created by OCLINT_CORETargets.cmake
set(OCLINT_RULES_LIBRARIES OCLintAbstractRule OCLintHelper OCLintUtil)
