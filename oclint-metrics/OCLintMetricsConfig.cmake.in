# - Config file for the OCLINT_CORE package
# It defines the following variables
#  OCLINT_METRICS_INCLUDE_DIRS - include directories for OCLINT_CORE
#  OCLINT_METRICS_LIBRARIES    - libraries to link against
#  OCLINT_METRICS_EXECUTABLE   - the bar executable
 
# Compute paths
get_filename_component(OCLINT_METRICS_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(OCLINT_METRICS_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")
 
# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET OCLintMetric AND NOT OCLINT_METRICS_BINARY_DIR)
  include("${OCLINT_METRICS_CMAKE_DIR}/OCLintMetricsTargets.cmake")
endif()
 
# These are IMPORTED targets created by OCLINT_CORETargets.cmake
set(OCLINT_METRICS_LIBRARIES OCLintMetric)
